{
  "name": "fusionclient",
  "author": "Alpha Upstream Corporation",
  "description": "A Minecraft client for the best experience.",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "render": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --publish always --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,svelte",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "svelte-check --tsconfig packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 npx --no-install electron ./scripts/update-electron-vendors.js "
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "cross-env": "7.0.3",
    "electron": "20.0.2",
    "electron-builder": "23.1.0",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.20.0",
    "happy-dom": "6.0.3",
    "nano-staged": "0.8.0",
    "playwright": "^1.23.4",
    "svelte": "^3.49.0",
    "svelte-check": "^2.2.8",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.3.1",
    "typescript": "4.7.4",
    "unplugin": "^0.8.1",
    "unplugin-auto-expose": "^0.0.1",
    "vite": "3.0.4",
    "vitest": "0.18.1"
  },
  "dependencies": {
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@rgossiaux/svelte-heroicons": "^0.1.2",
    "@smui/linear-progress": "^6.0.0",
    "@smui/select": "^6.0.0",
    "@smui/slider": "^6.0.0",
    "@typescript-eslint/parser": "^5.30.6",
    "@xmcl/core": "^2.10.1",
    "@xmcl/installer": "^4.4.0",
    "async": "^3.2.4",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "core-js": "^3.24.0",
    "decompress": "^4.2.1",
    "discord-rpc-patch": "^4.0.1",
    "dotenv": "^16.0.1",
    "electron-updater": "5.0.5",
    "fs-extra": "^10.1.0",
    "got": "^12.1.0",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "minecraft-data": "^3.5.1",
    "minecraft-launcher-core": "^3.16.13",
    "minecraft-server-util": "^5.3.0",
    "msmc": "^3.1.3",
    "nconf": "^0.12.0",
    "ngrok": "^4.3.1",
    "node-curseforge": "1.1.0",
    "nouislider": "^15.6.0",
    "os": "^0.1.2",
    "postcss": "8.4.14",
    "svelte-material-ui": "^6.0.0",
    "svelte-select": "^4.4.7",
    "svelte-virtual-list": "^3.0.1",
    "tailwindcss": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AarushX/Fusion.git"
  }
}
