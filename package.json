{
  "name": "fusion",
  "author": "Starframe Alpha Corporation",
  "description": "A Minecraft client for the best experience.",
  "private": true,
  "version": "1.1.0",
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "start": "pnpm i && echo Running Loader && pnpm watch",
    "build": "pnpm build:main && pnpm build:preload && pnpm build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production pnpm build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "render": "cross-env MODE=production pnpm build && electron-builder build --config .electron-builder.config.js --publish always --config.asar=false",
    "test": "pnpm test:main && pnpm test:preload && pnpm test:renderer && pnpm test:e2e",
    "test:e2e": "pnpm build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "spool": "echo Loading pnpm && pnpm && echo Starting Process && pnpm watch",
    "lint": "eslint . --ext js,ts,svelte",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "svelte-check --tsconfig packages/renderer/tsconfig.json",
    "typecheck": "pnpm typecheck:main && pnpm typecheck:preload && pnpm typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 npx --no-install electron ./scripts/update-electron-vendors.js ",
    "pnpm": "pnpm add -g pnpm"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.6",
    "@smui/linear-progress": "^6.1.4",
    "@smui/slider": "^6.1.4",
    "@sveltejs/vite-plugin-svelte": "^1.4.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "attractions": "^3.7.0",
    "carbon-components-svelte": "^0.70.13",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "electron": "19.1.8",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.36.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-svelte3": "^4.0.0",
    "flowbite": "^1.6.4",
    "flowbite-svelte": "^0.28.11",
    "playwright": "^1.31.2",
    "sass": "^1.59.2",
    "svelte": "^3.56.0",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unplugin": "^0.8.1",
    "unplugin-auto-expose": "^0.0.1",
    "vite": "^3.2.5",
    "vitest": "^0.18.1"
  },
  "dependencies": {
    "@rgossiaux/svelte-heroicons": "^0.1.2",
    "@typescript-eslint/parser": "^5.54.1",
    "@xmcl/installer": "^4.4.0",
    "adm-zip": "^0.5.10",
    "async": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "core-js": "^3.29.0",
    "decompress": "^4.2.1",
    "discord-rpc-patch": "^4.0.1",
    "dotenv": "^16.0.3",
    "electron-updater": "^5.3.0",
    "fs-extra": "^10.1.0",
    "got": "^12.6.0",
    "klaw": "^4.1.0",
    "lodash": "^4.17.21",
    "minecraft-data": "^3.30.0",
    "minecraft-launcher-core": "^3.16.18",
    "minecraft-server-util": "^5.4.0",
    "msmc": "^3.1.5",
    "ngrok": "^4.3.3",
    "node-curseforge": "^1.2.2",
    "node-json-db": "^2.1.4",
    "os": "^0.1.2",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.21",
    "skinview3d": "3.0.0-alpha.1",
    "svelte-material-ui": "^6.2.0",
    "svelte-virtual-list": "^3.0.1",
    "tailwindcss": "^3.2.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlphaUpstream/Fusion.git"
  }
}
